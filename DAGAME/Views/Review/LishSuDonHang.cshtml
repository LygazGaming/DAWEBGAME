@model IEnumerable<DAGAME.Models.EF.Order>
    @if (Model!=null)
    {
        var i = 0;
                <h2>Lịch sử mua hàng</h2>
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <td>STT</td>
                            <td>Mã DH</td>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach(var item in Model)
                {
                    <tr>
                        <td>@i</td>
                        <td>@item.Code</td>
                    </tr>
                            i++;
                        }
                    </tbody>
                </table>

            }
